{
  "name": "arza",
  "version": "1.1.0",
  "description": "Monorepo for the Arza project",
  "bin": "server/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node client/scripts/serve.js",
    "build": "cross-env NODE_ENV=production node client/scripts/build.js",
    "package": "npm run build && pkg . --output \"binaries/$npm_package_name\" && cp ./node_modules/open/xdg-open ./binaries/xdg-open",
    "version": "npm run build && git add client/build",
    "postversion": "git push && git push --tags"
  },
  "pkg": {
    "assets": [
      "client/build/**/*"
    ],
    "outputPath": "binaries",
    "targets": [
      "node14-macos",
      "node14-win"
    ]
  },
  "engines": {
    "node": ">=14.17"
  },
  "aliases": {
    "abstractions": "src/abstractions",
    "utils": "src/utils"
  },
  "author": "Arnaud Juracek",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "autoprefixer": "^7.1.2",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.18.13",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "fs-extra": "^5.0.0",
    "ghp": "^1.6.0",
    "handlebars": "^4.0.10",
    "kool-shell": "^1.5.0",
    "node-sass": "^4.5.3",
    "pkg": "^5.6.0",
    "postcss-loader": "^2.0.6",
    "progress-bar-webpack-plugin": "^1.10.0",
    "prop-types": "^15.6.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.4.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "express": "^4.18.1",
    "howler": "^2.2.1",
    "missing-math": "^3.2.0",
    "open": "^8.4.0"
  },
  "private": false,
  "homepage": "https://github.com/chevalvert/arza#readme",
  "bugs": {
    "url": "https://github.com/chevalvert/arza/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:chevalvert/arza.git"
  }
}
